{
  "name": "online-store",
  "version": "1.0.0",
  "description": "Online store with React frontend and Express backend",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "node build.js",
    "vercel-build": "node build.js"
  },
  "keywords": [
    "react",
    "express",
    "mongodb",
    "ecommerce"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}